10 SCREEN 0:WIDTH80:CLS
20 OPEN "font.asc" FOR OUTPUT AS #1
30 PRINT #1,"fontdata ; {{{"
40 FOR C=ASC("0") TO ASC("z")
41 IF C=ASC("[") THEN C=ASC("a")
42 IF C=ASC("9")+1 THEN C=ASC("A")
50 PRINT CHR$(C);
60 B=4096+8*C
70 PRINT #1," ;  ";CHR$(C)
80 FOR X=0 TO 7
81 D=VPEEK(B+X)
82 REM  D=D OR ((D AND 248)/2) :REM  OR ((D AND 127)*2)
83 REM D=D OR ((D AND 127)*2)
84 REM IF X<7 AND X>3 THEN D=D OR VPEEK(B+X+1)
89 VPOKE B+X,D
90 PRINT #1," db "+RIGHT$("00000000"+BIN$(D),8)
100 NEXT
110 PRINT #1,""
120 NEXT
130 PRINT #1," ; }}}"
140 CLOSE #1
